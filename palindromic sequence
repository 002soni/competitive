def isPalindrome(str): 
    z = int(len(str)/2)
    # Run loop from 0 to len/2  
    for i in range(0, z):  
        if str[i] != str[len(str)-i-1]: 
            return False
    return True
  
# main function 
s = ("malayalam")
ans = isPalindrome(s) 
  
if (ans): 
    print("Yes") 
else: 
    print("No") 
